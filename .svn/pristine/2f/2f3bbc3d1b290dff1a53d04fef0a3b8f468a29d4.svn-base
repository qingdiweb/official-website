*{
    margin:0;
    padding:0;
}
.common-sec{
    width: 100%;
    height: 100vh;
}

.common-sec .huaziHeader{
    width: 100%;
    height: 78px;
    margin-top: 20px;
}
.common-sec .huaziHeader .huaziHeader-logo{
    float: left;
    margin:11px 10px 11px 22px;
    width: 56px;
    height: 56px;
    background: url('img/huaziheader.png') no-repeat;
    background-size:100% 100%; 
}
.common-sec .huaziHeader .huaziHeader-text{
    float: left;
    margin-top:23px;
    width: 108px;
    height: 28px;
    background: url('img/huaziheader-txt.png') no-repeat;
    background-size:100% 100%; 
}
.common-sec .huaziHeader .huaziHeader-computer{
    float: right;
    width: 40px;
    height: 40px;
    margin:20px 20px 0px 0px;
    background: url('img/computer.png') no-repeat;
    background-size:100% 100%; 
}
.common-sec .huaziHeader .huaziHeader-computer-text{
    float: right;
    display: none;
    width:120px;
    height: 33px;
     margin:20px 20px 0px 0px;
    line-height: 33px;
    color: rgba(255, 255, 255, 1);
    font-size: 24px;
    text-align: center;
    font-family: STYuanti-SC-Regular;
    opacity: 0;
    transition: opacity 0.5s;
    -webkit-transition: opacity 0.5s;
    overflow: hidden;
    
}
.common-sec .huaziHeader .huaziHeader-computer-text:hover{
    opacity: 1;
   
}
.common-sec .sec-cont .left-sec-cont{
    float: left;
    width: 600px;
    margin:162px 0px 0px 132px;
}
.common-sec .sec-cont .left-sec-cont .text-one{
    height: 70px;
    line-height: 70px;
    color: rgba(0, 39, 131, 1);
    font-size: 56px;
    text-align: left;
    font-family: PingFangSC-Semibold;
}
.common-sec .sec-cont .left-sec-cont .text-two{
    height: 50px;
    line-height: 50px;
    margin:10px 0px 0px 0px;
    color: rgba(255, 255, 255, 1);
    font-size: 36px;
    text-align: left;

}
.common-sec .sec-cont .left-sec-cont .text-three{
    height: 100px;
    line-height: 50px;
    color: rgba(255, 255, 255, 1);
    font-size: 36px;
    text-align: left;
}
.common-sec .sec-cont .left-sec-cont .download-box{
    width: 415px;
}
.common-sec .sec-cont .left-sec-cont .ios-download{
    padding: 7.5px 56px 7.5px 40px;
    border-radius: 100px;
    color: rgba(255, 255, 255, 1);
    font-size: 24px;
    margin-right: 22px;
    border: 2px solid rgba(255, 255, 255, 1);
    background: url('img/share-arrow.png') 140px center no-repeat;
    background-size:22px 22px; 

}
.common-sec .sec-cont .left-sec-cont .android-download{
    padding: 7.5px 38px 7.5px 17px;
    border-radius: 100px;
    color: rgba(255, 255, 255, 1);
    font-size: 24px;
    border: 2px solid rgba(255, 255, 255, 1);
    background: url('img/share-arrow.png') 160px center no-repeat;
    background-size:22px 22px; 
}
.common-sec .sec-cont .left-sec-cont .ios-code{
    display: none;
    position: relative;
    width: 285px;
    height: 285px;
    margin:20px 0px 0px 60px;
    background: url('img/erweima.png') center center no-repeat;
    background-size:100% 100%; 
}
.common-sec .sec-cont .left-sec-cont .shake{
    display:block;-webkit-animation:gd-shake 0.5s ease;-moz-animation:gd-shake 0.5s ease;animation:gd-shake 0.5s ease;
}
/* .common-sec .sec-cont .left-sec-cont .ios-download:hover .ios-code{} */
@keyframes gd-shake{
    0%{top:0;}
    50%{top:-250px;}
    100%{top:250px;}
}
