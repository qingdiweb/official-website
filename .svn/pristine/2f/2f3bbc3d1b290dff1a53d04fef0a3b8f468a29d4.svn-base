.focus-spot{
    position: fixed;
    right: 30px;
    top:50%;
    transform: translateY(-50%);
}
.focus-spot li{
    position: relative;
    width: 18px;
    height: 18px;
    margin-bottom:33px; 
    list-style: none;
    cursor: pointer;
    box-sizing: border-box;
    border: 1px solid rgba(135, 146, 255, 1);
    border-radius: 50%;
}
.focus-spot li a{
    position: absolute;
    top:50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 18px;
    height: 18px;
    border-radius: 50%;
    background-color: rgba(135, 146, 255, 1);
}
 .focus-spot .colorfocus a{
    position: absolute;
    top:50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 18px;
    height: 18px;
    border-radius: 50%;
    background-color: rgba(57, 113, 255, 1);
} 
.focus-spot li .sel{
    position: absolute;
    top:50%;
    left: 50%;
    transform: translate(-50%,-50%);
    width: 12px;
    height: 12px;
    border-radius: 50%;
   
}